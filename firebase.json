{
  "hosting": {
    "public": "./dist\\saininternet\\browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "error_page": {
      "404": "/index.html"
    },
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"

      },
      {
        "source": "/sitemap.xml",
        "destination": "/sitemap.xml"
      },
      {
        "source": "/robots.txt",
        "destination": "/robots.txt"
      }
    ]
  }
}
